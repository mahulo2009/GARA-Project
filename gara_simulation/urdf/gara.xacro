<?xml version="1.0"?>

<!-- TODO CHECK INERTIAL COLLISION AND FIXED JOINTS" -->

<robot name="gara" xmlns:xacro="http://ros.org/wiki/xacro">

	<xacro:include filename="$(find gara_simulation)/urdf/gara.gazebo" />

	<!-- Base Link -->
  <link name="base_link"></link>

	<!--Robot chassis Link-->
	<link name="chassis">

  	<visual>
			<geometry>
		  	<cylinder length="0.018" radius="0.257"/>
			</geometry>
  	</visual>

		<collision>
  			<geometry>
    			<cylinder length="0.018" radius="0.257"/>
  			</geometry>
		</collision>

		<inertial>
  			<mass value="20"/>
  			<inertia 	ixx="0.330785" ixy="0.0" ixz="0.0" 
								 	iyy="0.330785" iyz="0.0" 
									izz="0.66049"/>
		</inertial>

	</link>

	<!--Robot chassis Joint-->
  <joint name="base_link_joint" type="fixed">

		<origin xyz="0 0 0.125" rpy="0 0 0" />
    <parent link="base_link"/>
    <child link="chassis" />

  </joint>

	<!--Caster Link-->
	<link name="center_wheel">

		<visual>
				<geometry>
					<sphere radius="0.058"/>
				</geometry>
		</visual>

		<collision>
 			<geometry>
    			<sphere radius="0.058"/>
 			</geometry>
		</collision>

		<inertial>
 			<mass value="1"/>
 			<inertia 	ixx="0.001346" ixy="0.0" ixz="0.0" 
								iyy="0.001346" iyz="0.0" 
								izz="0.001346"/>
		</inertial>

	</link>

	<!--Caster Joint-->
	<joint name="center_wheel_joint" type="fixed">

		<origin xyz="0.14 0.0 -0.067" rpy="0 0 0"/>
		<parent link="chassis"/>
		<child link="center_wheel"/>	

	</joint>

	<!-- Left Wheel Link  -->
	<link name='left_wheel'>

		<visual>
			<origin xyz="0.0 0 0" rpy="0 1.5707 1.5707"/>
			<geometry>
				<cylinder radius="0.125" length="0.05"/>
			</geometry>
		</visual>

		<collision name='collision'>
			<origin xyz="0.0 0 0" rpy="0 1.5707 1.5707"/>
			<geometry>
				<cylinder radius="0.125" length="0.05"/>
			</geometry>
		</collision>

		<inertial>
			<origin xyz="0.0 0 0" rpy="0 1.5707 1.5707"/>
			<mass value="2"/>
			<inertia
			  ixx="0.008229" ixy="0" ixz="0"
			  iyy="0.008229" iyz="0"
			  izz="0.015625"
			/>
		</inertial>

	</link>

	<!-- Left Wheel Joint  -->
	<joint name="left_wheel_hinge" type="continuous">

		<axis xyz="0 1 0" rpy="0 0 0"/>
		<origin xyz="-0.182 0.285 0" rpy="0 0 0"/>
		<parent link="chassis"/>
		<child link="left_wheel"/>

	</joint>

	<!-- Right Wheel Link -->
	<link name='right_wheel'>

		<visual >
			<origin xyz="0.0 0 0" rpy="0 1.5707 1.5707"/>
			<geometry>
				<cylinder radius="0.125" length="0.05"/>
			</geometry>
		</visual>

		<collision name='collision'>
			<origin xyz="0.0 0 0" rpy="0 1.5707 1.5707"/>
			<geometry>
				<cylinder radius="0.125" length="0.05"/>
			</geometry>
		</collision>

		<inertial>
			<origin xyz="0.0 0 0" rpy="0 1.5707 1.5707"/>
			<mass value="2"/>
			<inertia
			  ixx="0.008229" ixy="0" ixz="0"
			  iyy="0.008229" iyz="0"
			  izz="0.015625"
			/>
		</inertial>

	</link>

	<!-- Right Wheel Joint -->
	<joint name="right_wheel_hinge" type="continuous" >

		<axis xyz="0 1 0" rpy="0 0 0"/>	
		<origin xyz="-0.182 -0.285 0" rpy="0 0 0"/>
		<parent link="chassis"/>
		<child link="right_wheel"/>

	</joint>

	<!-- Mast Link -->
	<link name='mast'>

		<visual name='mast_visual'>
			<origin xyz="0.0 0 -0.559" rpy="0 0 0"/>
			<geometry>
				<cylinder radius="0.02" length="1.1"/>
			</geometry>
		</visual>

		<collision name='collision'>
			<origin xyz="0.0 0 -0.559" rpy="0 0 0"/>
			<geometry>
				<cylinder radius="0.02" length="1.1"/>
			</geometry>
		</collision>

		<inertial>
			<mass value="2"/>
			<inertia
			  ixx="0.201867" ixy="0" ixz="0"
			  iyy="0.201867" iyz="0"
			  izz="0.0004"
			/>
		</inertial>

	</link>

	<!-- Mast Joint -->
	<joint name="mast_hinge" type="fixed" >

		<origin xyz="0 0 1.1" rpy="0 0 0"/>
		<parent link="chassis"/>
		<child link="mast"/>

	</joint>

	<!-- Camera Link -->
	<link name="camera_link">

		<visual>
  			<geometry>
					<box size="0.025 0.085 0.01"/>
  			</geometry>     
		</visual>

		<collision>
  			<origin xyz="0 0 0" rpy="0 0 0"/>
  			<geometry>
				<box size="0.025 0.085 0.01"/>
  			</geometry>
		</collision>

		<inertial>
  			<mass value="1e-5" />
  			<inertia 	ixx="1e-6" ixy="0" ixz="0" 
									iyy="1e-6" iyz="0" 
									izz="1e-6" />
		</inertial>

	</link>

	<!-- Camera Joint -->
	<joint name="camera_joint" type="fixed">

		<origin xyz="0.09 0 0.85" rpy="0 0 0"/>
		<parent link="chassis"/>
		<child link="camera_link"/>

	</joint>

	<!-- Camera RGBD Link -->
	<link name='camera_rgbd_frame'></link>

	<!-- Camera RGBD Joint -->
	<joint name="camera_rgbd_joint" type="fixed" >

		<origin xyz="0 0 0" rpy="-1.5707963267948966 0 -1.5707963267948966"/>
  	<parent link="camera_link"/>
    <child link="camera_rgbd_frame"/>

  </joint>

	<!-- Hokuyo Laser Link -->
	<link name="hokuyo_link">

  		<visual>
    			<origin xyz="0 0 0" rpy="0 0 0"/>
    			<geometry>
      			<mesh filename="package://gara_simulation/meshes/hokuyo.dae"/>
    			</geometry>
  		</visual>

  		<collision>
    			<geometry>
      			<box size="0.1 0.1 0.1"/>
    			</geometry>
  		</collision>

  		<inertial>
    			<mass value="1e-5" />
    			<inertia 	ixx="1e-6" ixy="0" ixz="0" 
										iyy="1e-6" iyz="0" 
										izz="1e-6" />
  		</inertial>

	</link>

 	<!-- Hokuyo Laser Joint -->
	<joint name="hokuyo_joint" type="fixed">

    	<axis xyz="0 1 0" />
  		<origin xyz="0 0 1.1" rpy="0 0 0"/>
  		<parent link="chassis"/>
  		<child link="hokuyo_link"/>

  </joint>

	<!-- Hokuyo Laser Link -->
	<link name="imu_link">

		<visual>
				<geometry>
					<box size="0.01 0.01 0.01"/>
				</geometry>
		</visual>

		<collision>
  			<geometry>
    			<box size="0.01 0.01 0.01"/>
  			</geometry>
		</collision>

		<inertial>
  			<mass value="1e-5" />
  			<inertia 	ixx="1e-6" ixy="0" ixz="0" 
									iyy="1e-6" iyz="0" 
									izz="1e-6" />
		</inertial>

	</link>

 	<!-- IMU Joint -->
	<joint name="imu_joint" type="fixed">

		<axis xyz="0 1 0" />
		<origin xyz="-0.2 0 0" rpy="0 0 0"/>
		<parent link="chassis"/>
		<child link="imu_link"/>

  </joint>

</robot>
