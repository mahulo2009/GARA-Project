<launch>
  <node pkg="gara_simulation" type="teleop" name="turtlebot_teleop_keyboard" output="screen">
    <param name="scale_linear" value="0.5" type="double"/>
    <param name="scale_angular" value="1.5" type="double"/>
    <remap from="turtlebot_teleop_keyboard/cmd_vel" to="gara/cmd_vel"/>
  </node>
</launch>
